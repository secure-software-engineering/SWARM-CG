{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Main Runner",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/main_runner.py",
            "cwd": "${workspaceFolder}/src",
            "args": [
                "--language",
                "python",
                "--tool",
                "pycg",
                "--benchmark_name",
                "pycg"
            ],
            "console": "integratedTerminal",
        },
        {
            "name": "Ollama Runner",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/target_tools/ollama/src/runner.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--language",
                "python",
                "--benchmark_path",
                "${workspaceFolder}/benchmarks/python/pycg",
                "--ollama_models",
                "llama3.1:70b",
                // "mistral:v0.3",
                "--openai_key",
                "apikey",
                "--ollama_url",
                "http://ssegpu.cs.upb.de:11434",
                // "",
                "--prompt_id",
                "questions_based_1",
                "--results_dir",
                "${workspaceFolder}/results_llm"
            ]
        },
        {
            "name": "PyCG Runner",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/target_tools/pycg/src/runner.py",
            "args": [
                "--language",
                "python",
                "--benchmark_path",
                "${workspaceFolder}/benchmarks",
                "--results_dir",
                "${workspaceFolder}/.scrapy/results_pycg"
            ],
            "console": "integratedTerminal",
        },
        {
            "name": "Result Analyzer",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/main_analyze_result.py",
            "args": [],
            "console": "integratedTerminal",
        }
    ]
}